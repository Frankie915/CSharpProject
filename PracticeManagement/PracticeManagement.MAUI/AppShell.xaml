<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PracticeManagement.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PracticeManagement.MAUI"
    xmlns:local2="clr-namespace:PracticeManagement.MAUI.Views"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Client"
        ContentTemplate="{DataTemplate local2:ClientView}"
        Route="Client"/>

    <ShellContent
        Title="Employee"
        ContentTemplate="{DataTemplate local2:EmployeeView}"
        Route="Employee" />

    <ShellContent
        Title="Projects"
        ContentTemplate="{DataTemplate local2:ProjectDetailView}"
        Route="ProjectDetail" />

    <ShellContent
        Title="Time Details"
        ContentTemplate="{DataTemplate local2:TimeDetailView}"
        Route="TimeDetail" />


</Shell>
